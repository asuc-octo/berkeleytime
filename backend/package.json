    "tsc-watch": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "imports": {
    "#src/*": "./build/*.js"
  },
  "lint-staged": {
    "*.js|*.ts|*.jsx|*.tsx|*.json": [
      "npm run format"
    ]
  },
  "main": "build/index.js",
  "name": "backend",
  "scripts": {
    "build": "rm -rf build && tsc",
    "dev": "NODE_ENV=dev tsc-watch --onSuccess \"node --enable-source-maps build\"",
