{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "arrow-parens": "off",
    "camelcase": "off",
    "indent": ["error", 2],
    "max-len": "off",
    "no-console": "warn",
    "no-else-return": "off",
    "no-param-reassign": "off",
    "prefer-destructuring": "off",
    "radix": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-one-expression-per-line": "off"
  }
}
