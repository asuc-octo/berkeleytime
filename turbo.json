{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    ".env"
  ],
  "ui": "stream",
  "tasks": {
    "build": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "generate": {
      "cache": false,
      "outputs": [
        "*/**/generated-types/**",
        "*/**/generated/**"
      ]
    },
    "lint": {
      "cache": false,
      "dependsOn": [
        "generate"
      ]
    },
    "type-check": {
      "cache": false,
      "dependsOn": [
        "generate"
      ]
    },
    "start": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "dev": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "cache": false,
      "persistent": true
    },
    "main": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "persistent": true
    }
  }
}
