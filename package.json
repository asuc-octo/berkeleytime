{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "start": "turbo run start --no-cache",
    "format": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "postinstall": "patch-package"
  },
  "engines": {
    "node": ">=21.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.1",
    "turbo": "^2.3.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "latest"
  },
  "name": "berkeleytime"
}
